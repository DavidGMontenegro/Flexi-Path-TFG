{
    "source": {
      "include": ["components"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "template": "node_modules/tui-jsdoc-template"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    }
  }
  